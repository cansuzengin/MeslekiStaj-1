@model MvcProject.Models.Entity.TBLDUYURU
@{
    ViewBag.Title = "DuyuruDetay";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<h2>Duyuru Detayı</h2>

@using (Html.BeginForm("DuyuruDetay", "Duyuru", FormMethod.Post))
{
    <div style=" margin-bottom:15px"></div>
    <div class="form-group">
        @Html.LabelFor(k => k.ID)
        @Html.TextBoxFor(k => k.ID, new { @class = "form-control" })
        <br />
        <label>Duyuru</label>
        @Html.TextBoxFor(k => k.DUYURU, new { @class = "form-control" })
        <br />
    </div>
}